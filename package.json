{
  "name": "enso-subgraph",
  "description": "Subgraph",
  "private": true,
  "engines": {
    "node": ">=10",
    "yarn": "^1.17.3"
  },
  "version": "0.1.0",
  "scripts": {
    "prepare:kovan": "./scripts/build-manifest.sh kovan",
    "prepare:remote": "./scripts/build-manifest.sh remote",
    "prepare:mainnet": "./scripts/build-manifest.sh mainnet",
    "codegen": "graph codegen",
    "build": "yarn prepare:remote && graph codegen && graph build",
    "build:kovan": "yarn prepare:kovan && graph codegen && graph build",
    "build:mainnet": "prepare:remote && graph codegen && graph build",
    "deploy:local": "graph deploy --node http://172.17.0.1:8020/ --ipfs http://172.17.0.1:5001 enso/enso-v1",
    "deploy:remote": "graph deploy --node http://3.136.83.61:8020 --ipfs http://3.136.83.61:5001 enso/enso-v1",
    "deploy:kovan": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ enso/enso-v1"
  },
  "devDependencies": {
    "@graphprotocol/graph-ts": "0.20.1",
    "@graphprotocol/graph-cli": "0.20.1",
    "@openzeppelin/contracts": "~3.3.0",
    "mustache": "^4.1.0"
  }
}
